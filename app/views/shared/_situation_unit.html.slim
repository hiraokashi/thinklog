/.panel.panel-default
/  .panel-body
/    - unless situation.given_time_feelings.nil?
/    - situation.given_time_feelings.each do |given_time_feeling|
/      a.btn.btn-danger[href="/automatic_thoughts/think/#{given_time_feeling.automatic_thought.id}" data-no-turbolink="true"]
/        i.icon-cancel-circle
/        = given_time_feeling.feeling.contents + ' (' + given_time_feeling.before_percentage.to_s  + '%)'
/
/  .alert.alert-default
/    .label.label-success
/      | いつ？
/    span
/      =situation.when
/    br
/    .label.label-success
/      | どこで？
/    span
/      =situation.where
/    br
/    .label.label-success
/      | だれと？
/    span
/      =situation.with_whom
/    br
/    .label.label-success
/      | なにをしていたとき？
/    span
/      =situation.what_have_you_been_doing
/
/    br
/    br
/    a.btn.btn-default.edit_situation[id="situation_id_#{situation.id}" href="#"]
/      | 内容を編集する
