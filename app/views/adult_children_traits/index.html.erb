<p id="notice"><%= notice %></p>

<h1>Listing Adult Children Traits</h1>

<table>
  <thead>
    <tr>
      <th>Contents</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @adult_children_traits.each do |adult_children_trait| %>
      <tr>
        <td><%= adult_children_trait.contents %></td>
        <td><%= link_to 'Show', adult_children_trait %></td>
        <td><%= link_to 'Edit', edit_adult_children_trait_path(adult_children_trait) %></td>
        <td><%= link_to 'Destroy', adult_children_trait, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

